      },
      "edgeToEdgeEnabled": true,
      "package": "com.maloick.monchatbotreactnative"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-asset"
    ],
    "extra": {
      "eas": {
        "projectId": "fbd47b99-f20c-420d-af58-25698a745d94"
      }
    }
